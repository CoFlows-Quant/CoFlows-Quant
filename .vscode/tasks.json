{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        
        {
            "label": "clean server",
            "command": "dotnet clean ./CoFlows.CE.sln",
            "type": "shell",
            "group": "build",
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build server",            
            "command": "dotnet build ./CoFlows.CE.sln",
            "type": "shell",
            "group": "build",
            //"dependsOn": "clean server",
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish server",            
            "command": "dotnet publish -c Release -f netcoreapp2.2 -o obj/Core/publish ./CoFlows.CE.sln",
            "type": "shell",
            "group": "build",
            //"dependsOn": "clean server",
            "problemMatcher": "$msCompile"
        }
    ]
}